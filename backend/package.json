{
    "name": "catkin-backend",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "license": "MIT",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "format": "prettier --config .prettierrc --write \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\"",
        "formattests": "prettier --config .prettierrc --write \"test/**/*.{ts,js}\"",
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
        "start:debug": "nodemon --config nodemon-debug.json",
        "postinstall": "npm run prestart:prod",
        "prestart:prod": "rimraf dist && npm run build",
        "start:prod": "node dist/main.js",
        "tslint": "tslint -p tsconfig.json -c tslint.json",
        "lint": "eslint . --ext .ts --max-warnings 95",
        "lint:fix": "eslint . --fix --ext .ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json --verbose --silent=false --runInBand"
    },
    "dependencies": {
        "@nestjs/common": "7.6.18",
        "@nestjs/config": "0.6.3",
        "@nestjs/core": "7.6.18",
        "@nestjs/graphql": "7.11.0",
        "@nestjs/mongoose": "7.2.4",
        "@nestjs/passport": "7.1.6",
        "@nestjs/platform-express": "7.6.18",
        "@nestjs/terminus": "7.2.0",
        "@newrelic/apollo-server-plugin": "0.2.0",
        "@recap.dev/client": "1.14.0",
        "@types/express": "4.17.8",
        "@types/helmet": "0.0.48",
        "@types/passport": "1.0.7",
        "@types/passport-jwt": "3.0.5",
        "apollo-server-express": "2.25.2",
        "class-transformer": "0.4.0",
        "class-validator": "0.13.1",
        "dotenv": "8.6.0",
        "graphql": "15.5.1",
        "graphql-tools": "7.0.5",
        "helmet": "4.6.0",
        "jwks-rsa": "1.12.3",
        "mongoose": "5.13.2",
        "newrelic": "7.5.1",
        "passport": "0.4.1",
        "passport-jwt": "4.0.0",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "6.6.7",
        "rxjs-compat": "6.6.7",
        "type-graphql": "1.1.1"
    },
    "devDependencies": {
        "@nestjs/testing": "7.6.18",
        "@types/jest": "26.0.24",
        "@types/node": "14.17.5",
        "@types/socket.io": "3.0.1",
        "@types/supertest": "2.0.11",
        "@typescript-eslint/eslint-plugin": "4.25.0",
        "@typescript-eslint/parser": "4.25.0",
        "concurrently": "6.2.0",
        "eslint": "7.27.0",
        "eslint-config-prettier": "7.2.0",
        "eslint-plugin-prettier": "3.4.0",
        "jest": "26.6.3",
        "mock-jwks": "1.0.1",
        "mongodb-memory-server": "6.9.6",
        "nock": "13.1.1",
        "nodemon": "2.0.9",
        "prettier": "2.3.2",
        "supertest": "6.1.3",
        "ts-jest": "26.5.6",
        "ts-node": "9.1.1",
        "tsconfig-paths": "3.10.1",
        "tslint": "6.1.3",
        "typescript": "4.3.5",
        "wait-on": "5.3.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "keywords": [
        "Nestjs",
        "Typescript",
        "GraphQL",
        "Nestjs GraphQL",
        "Agile"
    ]
}
